{
    "name": "rivalex/livewire-notify",
    "description": "Simple Livewire notifications system without any dependencies except TALL-stack.",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Belyaev",
            "email": "db@codespb.com",
            "homepage": "https://codespb.com"
        },
        {
            "name": "Alessandro Rivolta",
            "email": "info@rivalex.com",
            "homepage": "https://rivalex.com"
        }
    ],
    "homepage": "https://github.com/rivalex/livewire-notify",
    "keywords": ["Laravel", "LivewireNotify","Toast","Growl","Livewire"],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^v7.4.0",
        "livewire/livewire": "^v2.10.5",
        "pestphp/pest": "^v1.21.2",
        "pestphp/pest-plugin-laravel": "^v1.2.0",
        "pestphp/pest-plugin-livewire": "^v1.0.0",
        "orchestra/canvas": "^7.x-dev",
        "nunomaduro/collision": "^v6.2.0",
        "symfony/yaml": "v6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Rivalex\\LivewireNotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rivalex\\LivewireNotify\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rivalex\\LivewireNotify\\LivewireNotifyServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
